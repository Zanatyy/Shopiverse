  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>GS_DS_SSRS</DataSourceName>
        <CommandText>USE GS_DWH;

SELECT TOP 10
    dp.Subcategory_name,
    SUM(f.Sales) AS Total_Sales
FROM 
    Fact_Table f
JOIN 
    Dim_Product dp ON f.SK_Prod_ID = dp.SK_Prod_ID
GROUP BY 
    dp.Subcategory_name
ORDER BY 
    Total_Sales DESC;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
