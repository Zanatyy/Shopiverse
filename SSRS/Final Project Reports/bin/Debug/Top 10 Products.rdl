  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>GS_DS_SSRS</DataSourceName>
        <CommandText>USE GS_DWH;

SELECT TOP 10
    dp.Product_Name,
    SUM(f.Sales) AS Total_Sales
FROM 
    Fact_Table f
JOIN 
    Dim_Product dp ON f.SK_Prod_ID = dp.SK_Prod_ID
GROUP BY 
    dp.Product_Name
ORDER BY 
    Total_Sales DESC;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Product_Name">
          <DataField>Product_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Total_Sales">
          <DataField>Total_Sales</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
